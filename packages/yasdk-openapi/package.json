{
  "name": "yasdk-openapi",
  "version": "0.2.8",
  "repository": "github:mtth/yasdk",
  "description": "YASDK OpenAPI utilities",
  "keywords": [
    "openapi",
    "yasdk"
  ],
  "author": "Matthieu Monsch <mtth@apache.org>",
  "license": "MIT",
  "type": "module",
  "files": [
    "lib/"
  ],
  "exports": {
    ".": "./lib/index.js",
    "./preamble": "./lib/preamble/index.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./lib/*",
        "./lib/*/index.js"
      ]
    }
  },
  "scripts": {
    "build": "tsc -p src",
    "clean": "rm -rf lib node_modules out",
    "fix": "prettier --write",
    "prepare": "pnpm run build",
    "test": "vitest",
    "watch": "tsc -b -w src"
  },
  "dependencies": {
    "@opvious/stl-errors": "^0.15.8",
    "@opvious/stl-utils": "^0.15.8",
    "@stoplight/json-ref-resolver": "^3.1.5",
    "openapi-schema-validator": "^12.1.0",
    "openapi-types": "^12.1.0",
    "yaml": "^2.2.1"
  }
}
