{
  "name": "abaca-openapi",
  "version": "0.9.2",
  "repository": "github:opvious/abaca",
  "description": "Abaca OpenAPI utilities",
  "keywords": [
    "openapi",
    "abaca"
  ],
  "author": "Opvious Engineering <oss@opvious.io>",
  "license": "Apache-2.0",
